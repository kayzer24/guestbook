{% extends 'base.html.twig' %}

{% block title %}Conference Guestbook{% endblock %}

{% block body %}
    <h1>Give your feedback</h1>

    {% for conference in conferences %}
        <h4>{{ conference }}</h4>
        <p>
            <a href="{{ path('conference', {'id': conference.id}) }}">View</a>
        </p>
    {% endfor %}
{% endblock %}
